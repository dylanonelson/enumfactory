{
  "version": "0.1.0",
  "main": "dist/index.bundle.js",
  "scripts": {
    "build": "webpack --colors --progress",
    "build:test": "webpack --colors --progress --config=webpack.config.test.js",
    "start": "tmux split-window -h \"npm run watch:build:test\" && tmux split-window -v \"npm run watch:build\" && npm run watch:test:server",
    "test": "npm run build:test && npm run test:server && npm run test:browser",
    "test:server": "mocha ./test/dist/ --reporter=list",
    "test:browser": "open ./test/dist/index.html",
    "watch:build": "npm run build -- --watch",
    "watch:build:test": "npm run build:test -- --watch",
    "watch:test:server": "mocha ./test/dist/ --reporter=list -w"
  },
  "author": "Dylan Nelson",
  "license": "ISC",
  "name": "enumjs",
  "dependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-1": "^6.22.0",
    "chai": "^3.5.0",
    "check-types": "^7.1.5",
    "css-loader": "^0.26.1",
    "eslint": "^3.15.0",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^6.9.0",
    "mocha": "^3.2.0",
    "proxy-polyfill": "^0.1.6",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.3.0",
    "ws": "^2.0.2"
  }
}
